{header}
import SwiftUI

@available(iOS 13.0, *)
public extension Color {
    struct {name} {
{data}
    }

    static var {variable}: {name}.Type {
        {name}.self
    }
}

@available(iOS 13.0, *)
internal extension Color {
    static func create(_ key: ColorKey) -> Color {
        Color(key.rawValue, bundle: Bundle.module)
    }
}
